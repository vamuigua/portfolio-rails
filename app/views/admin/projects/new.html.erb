<div class="skinny_wrapper wrapper_padding">
	<h1>New Project</h1>
	<%= form_for @project, url:admin_projects_path do |f| %>
	<% if @project.errors.any? %>
		<h2><%= pluralize(@project.errors.count,"error")%> prevented this project from saving:</h2>
		<ul>
			<% @project.errors.full_messages.each do |msg|%>
			<li><%= msg %></li>
			<%end%>
		</ul>
	<%end%>
	
	<%= f.label :title %><br>
	<%= f.text_field :title%>

	<br>
	<br>

	<%= f.label :link,"Write your link here" %><br>
	<%= f.text_field :link%>

	<br>
	<br>

	<%= f.label :description %><br>
	<%= f.text_area :description%>

	<br>
	<br>

	
	<%= f.submit %>
	 

<%end%>
</div>